<div class="row page-title">
	<div class="small-12 column">
		<h1>{{ article.title }}</h1>
		<p class="post-timestamp">
			{{ article.created_at | date: "%B %d, %Y" }}
		</p>
	</div>
</div>
<div class="row">
    <div class="small-12 large-6 large-centered columns">
        {{ article.content }}
    </div>
</div>
